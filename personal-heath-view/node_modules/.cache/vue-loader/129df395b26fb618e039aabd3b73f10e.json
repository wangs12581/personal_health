 <div style=\"font-size: 12px;\">\r\n                    <span class=\"news-tags\">{{ news.tagName }}</span>\r\n                    <span style=\"margin-left: 10px;\">收藏于 {{ parseTime(news.createTime) }}</span>\r\n                </div>\r\n            </el-col>\r\n        </el-row>\r\n    </el-row>\r\n\r\n</template>\r\n<script>\r\nimport { timeAgo } from '@/utils/data'\r\nexport default {\r\n    data() {\r\n        return {\r\n            keyWord: '',\r\n            newsData: [],\r\n            total: 0\r\n        };\r\n    },\r\n    created() {\r\n        this.keyWordParse();\r\n        this.keyListener();\r\n    },\r\n    methods: {\r\n        newsItemClick(news) {\r\n            sessionStorage.setItem('newsInfo', JSON.stringify(news));\r\n            this.$router.push('/news-detail');\r\n        },\r\n        parseTime(time) {\r\n            return timeAgo(time);\r\n        },\r\n        nameClick(news) {\r\n            sessionStorage.setItem('newsInfo', JSON.stringify(news));\r\n            this.$router.push(`/news-detail`);\r\n        },\r\n        keyWordParse() {\r\n            this.keyWord = sessionStorage.getItem('keyWord');\r\n            this.fetchData();\r\n        },\r\n        keyListener() {\r\n            setInterval(() => {\r\n                const key = sessionStorage.getItem('keyWord');\r\n                // 未变化，不做改变\r\n                if (key === this.keyWord) {\r\n                    return;\r\n                } else {\r\n                    this.keyWord = key;\r\n                    this.fetchData();\r\n                }\r\n            }, 100);\r\n        },\r\n        async fetchData() {\r\n            try {\r\n                const newsQueryDto = { name: this.keyWord }\r\n                const response = await this.$axios.post(`/news/query`, newsQueryDto);\r\n                const { data } = response;\r\n                this.newsData = data.data;\r\n                this.total = data.total;\r\n            } catch (e) {\r\n                console.error(`搜索资讯列表加载异常：${e}`);\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n<style scoped lang=\"scss\">\r\n.news-tags {\r\n    display: inline-block;\r\n    padding: 2px 5px;\r\n    background-color: rgb(222, 243, 251);\r\n    color: #1d3cc4;\r\n    border-radius: 3px;\r\n}\r\n\r\n.new-item:hover {\r\n    background-color: rgb(248, 248, 248);\r\n}\r\n\r\n.new-item {\r\n    margin-bottom: 10px;\r\n    padding: 10px;\r\n    box-sizing: border-box;\r\n\r\n    img {\r\n        width: 100%;\r\n        height: 118px;\r\n        border-radius: 3px;\r\n    }\r\n}\r\n\r\n.news-title {\r\n    overflow: hidden;\r\n    /* 显示省略符号来代表被修剪的文本。 */\r\n    text-overflow: ellipsis;\r\n    /* 文本不换行 */\r\n    white-space: nowrap;\r\n}\r\n</style>"]}]}