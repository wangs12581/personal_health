,YAAA,QAAA;;AACA,YAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,eAAA,QAAA,GAAA,IAAA,CAAA,IAAA,CAAA,MAAA,KAAA,CAAA;AACA;AACA,OALA;AAMA,KAZA;;AAaA;AACA,IAAA,iBAAA,GAAA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,sBAAA,EAAA;AAAA,QAAA,MAAA,EAAA,KAAA,QAAA,CAAA;AAAA,OAAA,EAAA,IAAA,CAAA,QAAA,IAAA;AACA,cAAA;AAAA,UAAA;AAAA,YAAA,QAAA;;AACA,YAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,eAAA,QAAA,CAAA,OAAA,CAAA,CAAA,KAAA,QAAA,GAAA,MAAA,GAAA,QAAA;AACA,eAAA,QAAA,GAAA,CAAA,KAAA,QAAA;AACA;AACA,OANA;AAOA,KAtBA;;AAuBA,IAAA,aAAA,CAAA,IAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,cAAA;AACA,KA1BA;;AA2BA;AACA,IAAA,SAAA,CAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,KA9BA;;AA+BA,IAAA,cAAA,GAAA;AACA,YAAA,OAAA,GAAA,cAAA,CAAA,OAAA,CAAA,UAAA,CAAA;AACA,WAAA,QAAA,GAAA,IAAA,CAAA,KAAA,CAAA,OAAA,CAAA;AACA,WAAA,cAAA;AACA,KAnCA;;AAoCA;AACA,IAAA,cAAA,GAAA;AACA,YAAA,WAAA,GAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,aAAA,EAAA,WAAA,EAAA,IAAA,CAAA,QAAA,IAAA;AACA,cAAA;AAAA,UAAA;AAAA,YAAA,QAAA;;AACA,YAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,eAAA,WAAA,GAAA,IAAA,CAAA,IAAA;AACA;AACA,OALA;AAMA;;AA7CA;AAhBA,CAAA","sourcesContent":["<template>\r\n    <el-row style=\"width: 900px;margin: 0 auto;\">\r\n        <el-col :span=\"18\">\r\n            <div style=\"padding-right: 50px;box-sizing: border-box;\">\r\n                <div>\r\n                    <h1>{{ newsInfo.name }}</h1>\r\n                    <div\r\n                        style=\"font-size: 12px;background-color: rgb(250, 250, 250);padding: 16px 6px;border-radius: 5px;\">\r\n                        <span class=\"news-tags\">{{ newsInfo.tagName }}</span>\r\n                        <span style=\"margin-left: 10px;\">{{ parseTime(newsInfo.createTime) }}</span>\r\n                        <el-button style=\"margin-left: 20px;\" @click=\"saveNewsOperation\" class=\"customer\" size=\"mini\">{{\r\n                            !saveFlag ?\r\n                                '立即收藏' : '取消收藏' }}</el-button>\r\n                    </div>\r\n                    <div v-html=\"newsInfo.content\"></div>\r\n                </div>\r\n                <div>\r\n                    <Evaluations :contentId=\"newsInfo.id\" contentType=\"NEWS\" />\r\n                </div>\r\n            </div>\r\n        </el-col>\r\n        <el-col :span=\"6\">\r\n            <h3 style=\"padding: 10px 30px;\">资讯推荐</h3>\r\n            <el-col @click.native=\"newsItemClick(news)\" :span=\"24\" :key=\"index\" v-for=\"(news, index) in newsTopList\">\r\n                <div style=\"padding: 25px 30px;box-sizing: border-box;\">\r\n                    <img :src=\"news.cover\" :alt=\"news.name\" style=\"width: 100%;height: 118px;border-radius: 5px;\">\r\n                    <h3 class=\"news-title\">{{ news.name }}</h3>\r\n                    <div style=\"font-size: 12px;\">\r\n                        <span class=\"news-tags\">{{ news.tagName }}</span>\r\n                        <span style=\"margin-left: 10px;\">{{ parseTime(news.createTime) }}</span>\r\n                    </div>\r\n                </div>\r\n            </el-col>\r\n        </el-col>\r\n    </el-row>\r\n</template>\r\n<script>\r\nimport { timeAgo } from \"@/utils/data\"\r\nimport Evaluations from \"@/components/Evaluations.vue\";\r\nexport default {\r\n    components: { Evaluations },\r\n    name: \"NewsDetail\",\r\n    data() {\r\n        return {\r\n            newsInfo: {},\r\n            newsTopList: [],\r\n            saveFlag: false,\r\n            newsSaveList: []\r\n        }\r\n    },\r\n    created() {\r\n        // 第一件事：是不是拿到存起来的资讯数据？\r\n        this.getStorageInfo();\r\n        this.loadAllTopNews();\r\n    },\r\n    methods: {\r\n        loadSaveStatus() {\r\n            const newsSaveQueryDto = {\r\n                newsId: this.newsInfo.id\r\n            }\r\n            console.log(\"查询的参数：\" + JSON.stringify(newsSaveQueryDto));\r\n            this.$axios.post('/news-save/queryUser', newsSaveQueryDto).then(response => {\r\n                const { data } = response;\r\n                if (data.code === 200) {\r\n                    this.saveFlag = data.data.length !== 0;\r\n                }\r\n            })\r\n        },\r\n        // 收藏或取消收藏操作\r\n        saveNewsOperation() {\r\n            this.$axios.post('/news-save/operation', { newsId: this.newsInfo.id }).then(response => {\r\n                const { data } = response;\r\n                if (data.code === 200) {\r\n                    this.$message.success(!this.saveFlag ? '收藏成功' : '取消收藏成功');\r\n                    this.saveFlag = !this.saveFlag;\r\n                }\r\n            })\r\n        },\r\n        newsItemClick(news) {\r\n            this.newsInfo = news;\r\n            this.loadSaveStatus();\r\n        },\r\n        // 转换时间\r\n        parseTime(time) {\r\n            return timeAgo(time);\r\n        },\r\n        getStorageInfo() {\r\n            const newInfo = sessionStorage.getItem('newsInfo');\r\n            this.newsInfo = JSON.parse(newInfo);\r\n            this.loadSaveStatus();\r\n        },\r\n        // 查询推荐资讯\r\n        loadAllTopNews() {\r\n            const newQueryDto = { isTop: true };\r\n            this.$axios.post('/news/query', newQueryDto).then(response => {\r\n                const { data } = response;\r\n                if (data.code === 200) {\r\n                    this.newsTopList = data.data;\r\n                }\r\n            })\r\n        },\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.news-tags {\r\n    display: inline-block;\r\n    padding: 4px 10px;\r\n    //background-color: rgb(226, 242, 249);\r\n    color: #1d3cc4;\r\n    border-radius: 3px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/user"}]}